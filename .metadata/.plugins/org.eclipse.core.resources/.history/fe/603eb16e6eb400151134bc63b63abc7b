std::unique_ptr<ASTExprNode> Parser::ParseNumberExpr()
{
	auto node = make_unique<ASTNumberExprNode>(CurrentToken.number_value);

	//auto node = std::make_unique<ASTNumberExprNode>(CurrentToken.number_value);

	//auto node = std::unique_ptr<ASTNumberExprNode>(new ASTNumberExprNode(std::forward<))
	//CurrentToken = Lex.GetToken();
