std::unique_ptr<ASTExprNode> Parser::ParseNumberExpr()
{
	auto node = make_unique<ASTNumberExprNode>(CurrentToken.number_value);
	CurrentToken = Lex.GetToken();
	return std::move(node);
}


std::unique_ptr<ASTExprNode> Parser::ParseNumberExpr()
{

	return nullptr;
}

