class ASTCallExprNode: public ASTExprNode {
public:
	ASTCallExprNode();
	virtual ~ASTCallExprNode();

private:
	std::string FunctionName;
	std::vector<std::unique_ptr<ASTExprNode>> Args;
};

#endif /* ASTCALLEXPRNODE_H_ */
